    "@azure/storage-blob": "^12.28.0",
    "@google-cloud/storage": "^7.16.0",
    "adm-zip": "^0.5.16",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
