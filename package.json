{
  "name": "am-reports-hub",
  "version": "1.0.0",
  "description": "AM Reports Hub - College Management and Report Generation System",
  "main": "app.js",
  "scripts": {
    "start": "node --max-old-space-size=4096 app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "build": "echo 'No build step required for Node.js app'",
    "postinstall": "echo 'AM Reports Hub installed successfully'",
    "db:migrate": "node src/database/migrate-data.js",
